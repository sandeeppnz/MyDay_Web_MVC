<div class="remodal"
     data-remodal-id="DialogLoop"
     id="DialogComplete"
     data-remodal-options="hashTracking: false, closeOnOutsideClick: false, closeOnEscape: false">


    <i class="fa fa-check-circle fa-4x" aria-hidden="true"></i>

    <br /><h2>Thank you for participating in the MyDay survey.</h2>

    <button data-remodal-action="cancel" class="remodal-confirm btn btn-success" type="button" id="btnOk">:)</button>
      <br /><br />

</div>
<script>
    var taskType = '@Session["taskType"]';
    var demoFor = '@Session["DemoForUser"]';
    //alert("taskType: " + taskType);

    $('#btnOk').on("click", function (e) {
                e.preventDefault();
                window.location.href = "@Url.Action("LogOff", "Account")";
        if (taskType == "WAM" && demoFor == "WAM")
        {window.location.href = "@Url.Action("LoopSurvey","Survey3", new {demoFor = "WAM", taskType = "WAM"})"; }
        else if (taskType == "doctors" && demoFor == "MyDay")
        { window.location.href = "@Url.Action("LoopSurvey","Survey3", new {demoFor = "MyDay", taskType = "doctors"})"; }
        else if (taskType == "doctors" && demoFor == "MyDayV2")
        { window.location.href = "@Url.Action("LoopSurvey","Survey3", new {demoFor = "MyDayV2", taskType = "doctors"})";}
        else { window.location.href = "@Url.Action("LoopSurvey","Survey3", new {demoFor = "MyDay", taskType = "Patient"})"; }

                @*window.location.href = "@Url.Action("LoopSurvey", "Survey3")";*@
            });

</script>
