@{
    Layout = null;
    ViewBag.Title = "Session Error";

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Need to Register - @SANSurveyWebAPI.Constants.AppHeader</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Login2.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />


</head>
<body class="loginBody">
    <div class="container TextAlignCenter">

        <div class="col-lg-4 col-md-3 col-sm-2"></div>
        <div class="col-lg-4 col-md-6 col-sm-8">
            <div class="row loginbox">

                <h1>
                    <span class="fa fa-exclamation-triangle" />
                </h1>

                <h3>
                    You are unable to login as you need to register first.
                </h3>
                Your invitation to registration is available in your email. <br />
                If you still face difficulty finding it, please contact us. <br /><br />
                You can close this page now. Thank you.


            </div>
            <br>
            <br>
            <footer class="footer">
                <p>© @DateTime.Now.Year @SANSurveyWebAPI.Constants.AppFooter </p>
            </footer> <!--footer Section ends-->


        </div>
    </div>



    <div class="col-lg-4 col-md-3 col-sm-2"></div>





</body>
</html>
