@{
    Layout = null;
    ViewBag.Title = "Screened Out";

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Screened out- @SANSurveyWebAPI.Constants.AppHeader</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Login2.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />


</head>
<body class="loginBody">
    <div class="container TextAlignCenter">

        <div class="col-lg-4 col-md-3 col-sm-2"></div>
        <div class="col-lg-4 col-md-6 col-sm-8">
            <div class="row loginbox">
                <h1>
                    <span class="fa fa-info-circle" />
                </h1>
                <h3>
                    Thank you for your willingness to participate in this survey.

                </h3>
                <h5>
                    However, @SANSurveyWebAPI.Constants.AppHeader survey is for respondents who are currently placed in a medical training program.
                    We will not proceed with your survey.
                </h5>


            </div>
            @Html.Partial("_Footer")


        </div>
    </div>



    <div class="col-lg-4 col-md-3 col-sm-2"></div>





</body>
</html>
