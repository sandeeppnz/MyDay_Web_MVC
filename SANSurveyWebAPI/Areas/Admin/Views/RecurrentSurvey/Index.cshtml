@model IEnumerable<SANSurveyWebAPI.DTOs.ProfileDto>



@{
    ViewBag.Title = "Recurrent Surveys";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";

}

<h2>Recurrent Survey Summary</h2>

<table class="table">
    <tr>

        <th>
            Profile Id
        </th>

        <th>
            Registration Progress
        </th>

      
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegistrationProgressNext)
            </td>
    

            <td>
                @Html.ActionLink("View", "RosterDetails", new { id = item.Id }) 
                @*@Html.ActionLink("Trace", "Details", new { id = item.Id })*@
            </td>
        </tr>
                    }

</table>
